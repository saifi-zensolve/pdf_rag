{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug - Launch FastAPI with Uvicorn",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--reload",
        "--host", "127.0.0.1",
        "--port", "8000"
      ],
      "preLaunchTask": "start-dependencies",
      "postDebugTask": "stop-dependencies",
      "env": {
        "QDRANT_URL": "http://localhost:6333",
        "LOG_LEVEL": "DEBUG",
        "ENVIRONMENT": "DEV"
      },
      "jinja": true
    }
  ]
}
