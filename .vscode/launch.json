{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug - Launch FastAPI with Uvicorn",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--reload",
        "--host", "127.0.0.1",
        "--port", "8000"
      ],
      "envFile": "${workspaceFolder}/.env.launch",
      "preLaunchTask": "start-dependencies",
      "postDebugTask": "stop-dependencies",
      "jinja": true
    }
  ]
}
